<?xml version="1.0" encoding="utf-8"?>
<!--形状-->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <solid android:color="#ffffff"/>
    <!--边框-->
    <stroke
        android:width="2dp"
        android:color="#fd8001"/>
    <!--内填充颜色大小-->
    <padding
        android:bottom="8dp"
        android:left="8dp"
        android:right="8dp"
        android:top="8dp"/>
    <!--圆角-->
    <corners android:radius="10dp"/>
</shape>
